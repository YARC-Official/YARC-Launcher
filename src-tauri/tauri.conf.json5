{
  "build": {
    "beforeDevCommand": "npm run front:dev",
    "beforeBuildCommand": "npm run front:build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": [
      "appimage",
      "msi",
      "app"
    ],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "createUpdaterArtifacts": "v1Compatible"
  },
  "productName": "YARC Launcher",
  "mainBinaryName": "YARC Launcher",
  "version": "1.1.0",
  "identifier": "in.yarg.launcher",
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDczMjdBMkM2RjYzRkExQ0MKUldUTW9ULzJ4cUluYzJTNG9VUis2azlaejYzcmc1SHRzT1Rtc0RUVWVCNEY2T0V1djlReENMV2gK",
      "endpoints": [
        "https://releases.yarg.in/launcher/update.json"
      ],
      "windows": {
        "installMode": "passive"
      }
    }
  },
  "app": {
    "withGlobalTauri": false,
    "windows": [
      {
        "label": "main",
        "fullscreen": false,
        "resizable": true,
        "title": "YARC Launcher",
        "width": 1300,
        "height": 800,
        "minWidth": 1200,
        "minHeight": 575,
        "decorations": false,
        "useHttpsScheme": true
      }
    ],
    "security": {
      "csp": null
    }
  }
}
